#include <stdio.h>
#define TAM 7

//Usando int para inverter numero

main(){
	int x, y, num = 0, primeira = 0;
	int armazena[TAM] = { };
    
    
    printf("Numero:\n");
    scanf("%d",&num);
    
    for(x = 0;x < TAM;x++){  	
    	armazena[x]= num%10;
    	num = num/10;
    	
    	if(num < 10){
    	   if(num == 0){
    	 	 primeira = x +1; //pq x comeÃ§a em 0
    	     break;
    	   }
    	  }
	}
	
	printf("tam %d \n",primeira);
	
	for(y = 0; y < primeira; y++)
		printf("%d", armazena[y]);
	  
}
